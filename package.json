  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "start": "npm-run-all --parallel start:*",
    "dev": "npm-run-all --parallel dev:*",
    "soap": "node script/build.js",
    "build:server": "nest build --webpack",
    "build:view-server": "tsc -p tsconfig.build-view.json && babel --no-babelrc --config-file ./.babelrc-server -d dist dist",
    "build:view": "cross-env NODE_ENV=production next build",
    "start:server": "cross-env NODE_ENV=production node --require reflect-metadata dist/main.js",
    "start:view": "cross-env NODE_ENV=production node dist/src/server.js",
    "dev:server": "nest start --webpack --watch",
    "dev:view": "nodemon server.ts --watch server.ts",
    "typeorm": "cross-env NODE_ENV=production ts-node -r tsconfig-paths/register --project ./tsconfig.server.json ./node_modules/typeorm/cli.js --config src/ormconfig.ts",
    "typeorm:migrate": "npm run typeorm migration:generate -- -n",
    "typeorm:run": "npm run typeorm migration:run",
    "typeorm:migrate:create": "npm run typeorm migration:create -- -n",
    "format": "prettier --write \"src/**/*.ts\"",
    "env": "sync-dotenv",
    "storybook": "start-storybook",
    "lint": "eslint src/** lib/** pages/** common/** --ext .ts,.tsx --ignore-pattern .eslintignore",
    "lint:fix": "eslint src/** lib/** pages/** common/** --ext .ts,.tsx --fix",
    "commitlint": "commitlint -E HUSKY_GIT_PARAMS",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -w -r 0",
    "release": "standard-version"
  },
